---
import MainLayout from "@/layouts/MainLayout.astro";
import Event from "@components/sections/misc/FAQ.astro";
import schedule from "@data/events-overall.json";
import exhibits from "@data/events-exhibits.json";
import HeroSection from "@components/sections/landing/HeroSection.astro";
import heroImage from "@images/building-map.avif";
const heroImages = [heroImage]
---
<MainLayout>
    <HeroSection
    title=`Build Events Map`
    primaryBtn="View Large"
    primaryBtnURL="/building-map.avif"
    src={heroImages}
    alt="Nerd Fest Building Map"
  />
    <Event title="Schedule" items={schedule} />
    <Event title="Exhibitors" items={exhibits} />
</MainLayout>